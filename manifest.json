{
  "name": "gitGrass Customizer",
  "author": "JominJun",
  "description": "Customizes contribution index of github.",
  "icons": {
    "128": "icon.png"
  },
  "version": "0.0.1",
  "manifest_version": 2,
  "background": { "scripts": ["background.js"] },
  "permissions": ["tabs", "webNavigation", "storage", "*://github.com/*"],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "settings.html"
  }
}
